mixin s-contact()
	// start s-contact
	+b.s-contact#s-contact&attributes(attributes)
		.container
			.row
				.col-lg-6
					.section
						+e.title.text-primary Контактная информация
						.h2.strong
							+e.link--lg(href="tel:8-800-350-48-59") 8-800-350-48-59
						.h4
							+e.link(href="tel:+7(391)222-09-04") +7 (391) 222-09-04
							br
							+e.link(href="tel:+7(383)304-85-99") +7 (383) 304-85-99
						.h4
							+e.link--mail(href="tel:krepostgrupp@mail.ru") krepostgrupp@mail.ru
						.h4 г. Красноярск, 
						p ул. Свердловская, 3&nbsp;&laquo;Д&raquo;, офис 409
						  
				.col
					script.
						var map;
						function initMap() {
						map = new google.maps.Map(document.getElementById('map'), {
						zoom: 15,
						//- center: new google.maps.LatLng(53.98205856875258,88.80421049999992),
						center: new google.maps.LatLng(55.98205856875258,92.88421049999992),
						mapTypeId: 'roadmap',
						styles: [
									{
											"featureType": "all",
											"elementType": "all",
											"stylers": [
													{
															"hue": "#ff0000"
													},
													{
															"gamma": "0.00"
													},
													{
															"lightness": "0"
													},
													{
															"saturation": "1"
													}
											]
									},
									{
											"featureType": "administrative",
											"elementType": "labels.text.fill",
											"stylers": [
													{
															"color": "#444444"
													}
											]
									},
									{
											"featureType": "landscape",
											"elementType": "all",
											"stylers": [
													{
															"color": "#f2f2f2"
													}
											]
									},
									{
											"featureType": "poi",
											"elementType": "all",
											"stylers": [
													{
															"visibility": "off"
													}
											]
									},
									{
											"featureType": "road",
											"elementType": "all",
											"stylers": [
													{
															"saturation": -100
													},
													{
															"lightness": 45
													}
											]
									},
									{
											"featureType": "road.highway",
											"elementType": "all",
											"stylers": [
													{
															"visibility": "simplified"
													}
											]
									},
									{
											"featureType": "road.arterial",
											"elementType": "labels.icon",
											"stylers": [
													{
															"visibility": "off"
													}
											]
									},
									{
											"featureType": "transit",
											"elementType": "all",
											"stylers": [
													{
															"visibility": "off"
													}
											]
									},
									{
											"featureType": "water",
											"elementType": "all",
											"stylers": [
													{
															"color": "#ffffff"
													},
													{
															"visibility": "on"
													}
											]
									}
							]
						});
						var iconBase = 'img/mark.png';
						var icons = {
					 
						info: {
						icon: iconBase
						}
						};
						var features = [
						{
						position: new google.maps.LatLng(55.98205856875258,92.88421049999992),
						type: 'info',
						title: 'г. Красноярск',
						}, 
						//- {
						//- position: new google.maps.LatLng(54.999397069703306,82.95985649999986),
						//- type: 'info',
						//- title: 'г. Новосибирск',
						//- },  
						];
						// Create markers.
						features.forEach(function(feature) {
						var marker = new google.maps.Marker({
						position: feature.position,
						icon: icons[feature.type].icon,
						map: map,
						title: feature.title,
						//- title: title
						});
						});
						}
					//- script(async='', defer='', src='https://maps.googleapis.com/maps/api/js?key=key=AIzaSyCnoRp8wBQeCZLL8k4R-vckP_wDGVR2k10&callback=initMap')
					script(async='', defer='' src='https://maps.googleapis.com/maps/api/js?key=AIzaSyCnoRp8wBQeCZLL8k4R-vckP_wDGVR2k10&callback=initMap')
					+e.map-wrap
						#map
		block
	// end s-contact